{
  "welcomeFile": "painter/webapp/index.html",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/painter/webapp/(.*)$",
      "target": "/painter/webapp/$1",
      "destination": "ui",
      "authenticationType": "none"      
    },

    {
      "source": "^/(.*)$",
      "target": "/$1",
      "destination": "backend",
      "authenticationType": "none",
      "cacheControl": "no-cache, no-store, must-revalidate"
    }

  ]
}