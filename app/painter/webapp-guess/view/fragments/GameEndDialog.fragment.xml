<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
   <Dialog id="endDialog" title="{i18n>endGameTitle}">
      <VBox>
         <Image src="resources/img/splash.svg" />
         <FlexBox alignItems="Center" justifyContent="Center">
            <items>
               <Button type="Accept" width="230px" press=".onFinishGame" text="{i18n>endGameButton}" />
            </items>
         </FlexBox>
      </VBox>
   </Dialog>
</core:FragmentDefinition>